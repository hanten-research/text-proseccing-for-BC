<xml xmlns="https://developers.google.com/blockly/xml" id="toolbox" style="display: none">
  <category name="文字と文章・数字" colour="#5b80a5">
    <block type="text">
      <field name="TEXT"></field>
    </block>
    <block type="math_number">
      <field name="NUM">0</field>
    </block>
    <block type="neko"></block>
  </category>
  <category name="変数" colour="#a55b80" custom="VARIABLE"></category>
  <category name="文章の処理" colour="#745ba5">
    <block type="separate" deletable="false" movable="false" editable="false"></block>
    <block type="replace" deletable="false" movable="false" editable="false"></block>
    <block type="remove" deletable="false" movable="false" editable="false"></block>
  </category>
  <category name="文章のリストの処理" colour="#a5745b">
    <block type="search_word" deletable="false" movable="false" editable="false"></block>
    <block type="search_num" deletable="false" movable="false" editable="false"></block>
    <block type="length" deletable="false" movable="false" editable="false"></block>
  </category>
  <category name="表示" colour="#9fa55b">
    <block type="console_log" deletable="false" movable="false" editable="false"></block>
  </category>
</xml>